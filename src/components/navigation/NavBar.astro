---
import { Image } from 'astro:assets';
import PageSearch from './PageSearch.astro';

import compositorLogo from '../../assets/compositor-logo.svg';
import discordLogo from '../../assets/discord-logo.svg';
import githubLogo from '../../assets/github-logo.svg';
---

<header>
  <nav class="flex h-20 justify-between px-20">
    <Image src={compositorLogo} alt="Compositor logo" class="fill-nav-icon" />
    <div class="flex gap-x-6">
      <PageSearch />
      <Image src={discordLogo} alt="Discord logo" class="fill-nav-icon" />
      <Image src={githubLogo} alt="Github logo" class="fill-nav-icon" />
    </div>
  </nav>
</header>
