---
import StreamSection from "../../components/demos/streaming-app/StreamSection";
import DemosLayout from "../../layouts/DemosLayout.astro";
---

<DemosLayout
  canonicalUrl="https://smelter.dev/demos/streaming-app"
  keywords="video compositing, react live stream, react video compositing, compositing software, real time video editing, live video editing, video mixing, live stream mixing"
  description="Low-latency video compositing tool with seamless developer experience. Easily compose live streams and videos with ready-to-go React components."
>
  <main class="flex items-center min-h-screen w-full overflow-clip py-12 px-16">
    <div class="h-full flex flex-1 flex-col">
      <div
        class="w-full max-w-[1400px] relative gap-y-4 flex flex-col self-center"
      >
        <StreamSection client:load />
      </div>
    </div>
  </main>
</DemosLayout>
